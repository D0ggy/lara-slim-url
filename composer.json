{
    "name": "d0ggy/lara-slim-url",
    "description": "easy slim url",
    "type": "library",
    "keywords": ["laravel", "url"],
    "license": "MIT",
    "version": "0.1.0-alpha",
    "authors": [
        {
            "name": "赵泽仁",
            "email": "dog_z@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": ">=5.5",
        "squizlabs/php_codesniffer": "*",
        "orchestra/testbench": "^3.5.0"
    },
    "autoload": {
        "psr-4": {
            "D0ggy\\LaraSlimUrl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D0ggy\\LaraSlimUrl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "scripts": {
            "test": "./vendor/bin/phpunit"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "D0ggy\\LaraSlimUrl\\SlimUrlServiceProvider"
            ]
        },
        "aliases": {
            "SlimUrl": "D0ggy\\LaraSlimUrl\\Facades\\SlimUrl"
        }
    }
}
